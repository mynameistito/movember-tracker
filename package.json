{
	"name": "shaggedy-movember",
	"version": "1.0.0",
	"description": "Cloudflare Worker to scrape Movember donation amounts using HTML parsing",
	"main": "src/index.ts",
	"scripts": {
		"dev": "wrangler dev --remote",
		"deploy": "wrangler deploy",
		"typecheck": "tsc --noEmit",
		"tail": "bunx wrangler tail --format json",
		"check": "bunx @biomejs/biome check --write .",
		"format": "bunx @biomejs/biome format --write .",
		"tsc": "bunx tsc --noEmit"
	},
	"keywords": [
		"cloudflare",
		"workers",
		"scraper",
		"movember"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251106.1",
		"@biomejs/biome": "^2.3.4",
		"typescript": "^5.9.3",
		"wrangler": "^4.46.0"
	}
}
